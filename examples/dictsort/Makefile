# makefile

build: dictsort1-c dictsort1-cpp dictsort2-c dictsort2-cpp 

dictsort1-c: dictsort1.c
	gcc -o dictsort1-c dictsort1.c

dictsort1-cpp: dictsort1.cpp 
	g++ -o dictsort1-cpp dictsort1.cpp

dictsort2-c: dictsort2.c
	gcc -o dictsort2-c dictsort2.c

dictsort2-cpp: dictsort2.cpp 
	g++ -o dictsort2-cpp dictsort2.cpp

clean:
	rm dictsort1-c dictsort1-cpp dictsort2-c dictsort2-cpp 